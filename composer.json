{
    "name": "nebubit/aparse",
    "description": "A command line tool for PHP developers to analyze Apache log file.",
    "type": "tool",
    "keywords": [
        "parse",
        "console",
        "interactive",
        "shell",
        "apache access log analyzer"
    ],
    "require": {
        "php": ">=5.5",
        "psy/psysh": "~0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "3.*"
    },
    "bin": ["bin/aparse"],
    "autoload": {
        "psr-4": {
            "AParse\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Tan"
        }
    ]
}
